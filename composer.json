{
    "name": "enmarche/mailer-bundle",
    "license": "GPL-3.0",
    "description": "Mailer bundle to ease proxying SAAS among private applications.",
    "type": "symfony-bundle",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.1",
        "ramsey/uuid": "3.9.3",
        "symfony/dependency-injection": "4.4.16",
        "symfony/http-kernel": "4.4.16"
    },
    "require-dev": {
        "coduo/php-matcher": "3.2.3",
        "doctrine/doctrine-bundle": "1.12.12",
        "guzzlehttp/guzzle": "6.5.5",
        "php-amqplib/rabbitmq-bundle": "1.14.4",
        "phpunit/phpunit": "7.5.20",
        "ramsey/uuid-doctrine": "1.6.0"
    },
    "autoload": {
        "psr-4": {
            "EnMarche\\MailerBundle\\": "src",
            "EnMarche\\MailerBundle\\Behat\\Context\\": "behat/Context"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EnMarche\\MailerBundle\\Tests\\": "tests"
        }
    }
}
