<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <!-- Allow enabling "null" transport type in "config/packages/test/en_marche_mailer.yaml" -->
        <service id="en_marche_mailer.mailer.transporter.null" class="EnMarche\MailerBundle\Test\NullMailTransporter" public="false" />

        <service id="EnMarche\MailerBundle\MailPost\MailPostInterface" alias="en_marche_mailer.mail_post.default" public="true" />
    </services>
</container>
